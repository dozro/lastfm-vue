{
  //"extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "auto-imports.d.ts", "src/**/*.vue"],
  "compilerOptions": {
    "target": "es2024",
    "module": "es2022",
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "lib": [
        "ES2020"
    ]
  }
}
