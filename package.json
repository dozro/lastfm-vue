{
  "name": "@itsrye/project-angus-lastfm",
  "description": "a (incomplete) template for using lastfm in Vue",
  "author": "Rye <npm@itsrye.dev> (http://itsrye.dev/)",
  "version": "0.0.07",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "bugs": {
    "url": "https://github.com/dozro/lastfm-vue/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build:ts": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:webpack": "webpack --mode production",
    "build": "bun run build:webpack && bun run build:ts ",
    "build:tsup": "tsup src/index.ts --format esm,cjs --dts"
  },
  "files": [
    "dist/*",
    "dist/index.d.ts",
    "scr/*",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lib": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@nuxt/ui": "^4.1.0",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "ky": "^1.14.0",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@swc/register": "latest",
    "@vitest/eslint-plugin": "latest",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "latest",
    "@vue/tsconfig": "^0.8.1",
    "eslint-plugin-oxlint": "~1.28.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.5.0",
    "sass-loader": "^16.0.0",
    "ts-loader": "^9.0.0",
    "tsup": "^8.5.1",
    "typescript": "^6.0.0-dev.20251116",
    "vue-loader": "latest",
    "vue-tsc": "^3.1.1",
    "webpack": "^5.0.0",
    "webpack-cli": "^6.0.0"
  }
}
