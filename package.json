{
  "name": "@itsrye/project-angus-lastfm",
  "description": "a (incomplete) template for using lastfm in Vue",
  "author": "Rye <npm@itsrye.dev> (http://itsrye.dev/)",
  "version": "0.0.10-PREVIEW",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "bugs": {
    "url": "https://github.com/dozro/lastfm-vue/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build:ts": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:vuelib": "vite build --config vite.vue.lib.config.js",
    "build": "bun run build:ts && bun run build:vuelib"
  },
  "files": [
    "dist/*",
    "dist/index.d.ts",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue/*.vue.d.ts",
      "require": "./dist/angus_lastfm.cjs",
      "import": "./dist/angus_lastfm.esm.js"
    }
  },
  "dependencies": {
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "ky": "^1.14.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "^6.0.0-dev.20251116",
    "vite": "^7",
    "vite-plugin-dts": "^4",
    "vue-tsc": "^3"
  },
  "peerDependencies": {
    "@nuxt/ui": "^4.1.0",
    "vue": ">=3.3",
    "tailwindcss": "^4.1.17"
  }
}
